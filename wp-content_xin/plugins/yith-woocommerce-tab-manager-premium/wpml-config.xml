    </custom-types>
    <custom-fields>
        <custom-field action="copy">_ywtm_tab_type</custom-field>
        <custom-field action="copy">_ywtm_show_tab</custom-field>
        <custom-field action="copy">_ywtm_order_tab</custom-field>
        <custom-field action="copy">_ywtm_icon_tab</custom-field>
        <custom-field action="copy">_ywtm_layout_type</custom-field>
